<p id="notice"><%= notice %></p>

<p>
  <strong>Title:</strong>
  <%= @post.title %>
</p>

<p>
  <strong>Description:</strong>
  <%= @post.description %>
</p>


<% @post.image_attachments.each do |image| %>
  <%= attachment_image_tag(image, :file, :fill, 100, 100, format: "jpg") %>
  <%= link_to 'Vote', '#' %>
<% end %>
